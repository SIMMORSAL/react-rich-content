{
  "name": "react-rich-content",
  "version": "1.0.10",
  "description": "A frontend CMS solution that allows you to create beautiful, animated, and responsive pages easily and fast by supplying it a javascript object.",
  "type": "module",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SIMMORSAL/react-rich-content.git"
  },
  "keywords": [
    "CMS",
    "Rich",
    "Content",
    "React",
    "Animated",
    "Blog",
    "Article",
    "Text"
  ],
  "author": "SIMMORSAL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SIMMORSAL/react-rich-content/issues"
  },
  "homepage": "https://github.com/SIMMORSAL/react-rich-content#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.11.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^6",
    "react-player": "^2.11.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/react": "^18.0.27",
    "@types/react-syntax-highlighter": "^15.5.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.4.2",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
